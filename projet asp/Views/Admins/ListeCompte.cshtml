@model IEnumerable<projet_asp.Models.Admin>
@{
    ViewBag.Title = "ListeCompte";
}

<h2>ListeCompte</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Prenom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MotDePasse)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Role)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Prenom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MotDePasse)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Role)
            </td>
            <td>
                @Html.ActionLink(projet_asp.Resources.MyResource.edit, "Edit", new { id = item.Id }, new { @class = "btn btn-primary" })
                @Html.ActionLink(projet_asp.Resources.MyResource.details, "Details", new { id = item.Id }, new { @class = "btn btn-info" })
                @Html.ActionLink(projet_asp.Resources.MyResource.delete, "Delete", new { id = item.Id }, new { @class = "btn btn-danger btn-delet" })
            </td>
        </tr>
    }

</table>
<div>
    @Html.ActionLink(projet_asp.Resources.MyResource.back, "Index", "Directeurs", null, new { @class = "btn btn-default" })
</div>

